[run]
branch = True
source = 
    scripts
    memory_system
    projects/KnowledgeBuilder/src
    projects/SkillBuilder/skill_builder

omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */.venv/*
    */site-packages/*
    */node_modules/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    class .*\bProtocol\):
    @overload

[html]
directory = htmlcov
title = Chrysalis Test Coverage Report

[xml]
output = coverage.xml
